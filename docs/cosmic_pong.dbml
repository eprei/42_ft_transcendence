Table Player {
    id integer [pk]
    login text
    avatarUrl text
    nbVictory integer
    totalPlay integer
    xp integer
    TFASecret varchar
    TFAEnabled boolean
    FT_id varchar
}

Table Friends {
    id integer [pk]
    player integer
    friend integer
    isPending boolean
}

Table Match {
    id integer [pk]
    playerHome integer
    playerForeign integer
    Winner integer
    HomeScore integer
    ForeignScore integer
    creationDate date
}

Ref: Player.id < Friends.player
Ref: Player.id < Friends.friend

Ref: Player.id < Match.playerHome
Ref: Player.id < Match.playerForeign
Ref: Player.id < Match.Winner
