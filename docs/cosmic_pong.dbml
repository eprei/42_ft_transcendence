Table Player {
    id integer
    login text
    avatarUrl text
    nbVictory integer
    totalPlay integer
    xp integer
    TFASecret varchar
    TFAEnabled boolean
    FT_id varchar
}

Table Friends {
    id integer
    player integer
    friend integer
    isPending boolean
}

Table Match {
    id integer
    playerHome integer
    playerForeign integer
    Winner integer
    HomeScore integer
    ForeignScore integer
    creationDate date
}

Ref: Player.id < Friends.player
Ref: Player.id < Friends.friend
